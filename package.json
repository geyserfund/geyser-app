    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.38.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.2.1",
